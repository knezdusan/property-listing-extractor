    },
    "last_minute_booking": {
      "type": "boolean",
      "description": "Indicates if last-minute bookings are allowed",
      "optional": true,
      "source": "Airbnb’s booking preferences"
    },
    "check_in_window": {
      "type": "string",
      "description": "Check-in time window (e.g., '3 PM - 9 PM')",
      "optional": true
    },
    "check_out_window": {
      "type": "string",
      "description": "Check-out time window (e.g., '10 AM - 12 PM')",
      "optional": true
    }
  },
  "cancellation_policy": {
    "type": { "type": "string", "description": "Cancellation policy type (e.g., 'Strict')" },
