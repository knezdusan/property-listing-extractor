    "minimum_stay": { "type": "integer", "description": "Minimum nights required (e.g., 1)", "optional": true },
    "maximum_stay": { "type": "integer", "description": "Maximum nights allowed", "optional": true },
    "advance_notice": {
      "type": "string",
