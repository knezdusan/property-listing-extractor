{
  "listing": {
    "id": { "type": "string", "description": "Unique identifier for the listing (e.g., '30397973')" },
    "url": {
      "type": "string",
      "description": "Canonical URL of the listing (e.g., 'https://www.airbnb.com/rooms/30397973')"
    },
    "property_type": {
      "type": "string",
      "description": "Type of building or structure the listing is. eg.( 'apartment', 'house', 'bungalow', 'chalet', 'cabin', 'cottage', 'farmhouse', 'guesthouse', 'guestroom', 'houseboat', 'hut', 'igloo', 'loft', 'mansion', 'pavilion', 'shack', 'studio', 'tent', 'townhouse', 'villa', 'yurt' )"
    },
    "privacy_type": {
      "type": "string",
      "description": "what guests are renting (whole property or part) (e.g., 'Entire place', 'Private room', 'Shared room', 'Hotel room')"
    },
    "title": {
      "type": "string",
      "description": "Listing title (e.g., 'Eiffel Tower IN your living room')",
      "constraints": "max 50 characters"
    },
    "subtitle": { "type": "string", "description": "Listing subtitle (e.g., 'Entire rental unit in Berlin, Germany')" },
    "capacity_highlights": {
      "type": "array",
      "description": "Listing capacity highlights (e.g., ['3 guests', '1 bedroom', '2 beds', '1 bath'])",
      "items": { "type": "string" }
    },
    "highlights": {
      "type": "array",
      "description": "Key selling points or notable features highlighted for the listing (e.g., Superhost status, great check-in, location perks).",
      "optional": true,
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "The main text of the highlight (e.g., 'Great check-in experience')."
          },
          "subtitle": {
            "type": "string",
            "description": "Supporting detail or explanation for the highlight.",
            "optional": true
          }
        },
        "required": ["title"]
      }
    },
    "description": { "type": "text", "description": "Provides in-depth property listing info for guests" },
    "tags": {
      "type": "array",
      "description": "Custom tags for categorization (e.g., ['Romantic', 'Family-friendly', 'Business'])",
      "items": { "type": "string" },
      "optional": true
    }
  },
  "location": {
    "city": { "type": "string", "description": "City of the listing (e.g., 'Paris')" },
    "region": { "type": "string", "description": "Region or state (e.g., 'Île-de-France')" },
    "country": { "type": "string", "description": "Country (e.g., 'France')" },
    "zip_code": { "type": "string", "description": "Postal or ZIP code of the property", "optional": true },
    "coordinates": {
      "latitude": { "type": "number", "description": "Latitude of the property (e.g., 48.85068)" },
      "longitude": { "type": "number", "description": "Longitude of the property (e.g., 2.29188)" }
    },
    "neighborhood": {
      "description": "Detailed information about the neighborhood, including a preview and potentially extended sections like 'Getting Around'.",
      "optional": true,
      "properties": {
        "preview": {
          "type": "object",
          "description": "A concise summary or preview of the neighborhood, often displayed prominently.",
          "properties": {
            "title": { "type": "string", "description": "Title for the preview section (often the city/area name)." },
            "content": { "type": "string", "description": "Text content describing the neighborhood preview." }
          }
        },
        "extended": {
          "type": "array",
          "description": "An array of additional sections providing more details about the neighborhood or related topics like transportation.",
          "items": {
            "type": "object",
            "properties": {
              "title": { "type": "string", "description": "Title for this extended section (e.g., 'Getting around')." },
              "content": { "type": "string", "description": "Text content for this extended section." }
            }
          }
        }
      }
    }
  },
  "house_rules": {
    "type": "array",
    "items": { "title": "string", "subtitle": "string" },
    "description": "Rules guests must follow",
    "optional": true
  },
  "safety_features": {
    "type": "array",
    "description": "Categorized list of safety considerations, devices, and property info, reflecting the structure from 'safetyAndPropertiesSections'.",
    "optional": true,
    "items": {
      "type": "object",
      "properties": {
        "section_title": {
          "type": "string",
          "description": "The title of the safety/property category (e.g., 'Safety considerations', 'Safety devices', 'Property info'). Sourced from GeneralListContentSection.title."
        },
        "items": {
          "type": "array",
          "description": "List of specific safety/property items within this category. Sourced from GeneralListContentSection.items.",
          "items": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string",
                "description": "The specific safety/property item title (e.g., 'Smoke alarm installed'). Sourced from BasicListItem.title."
              },
              "subtitle": {
                "type": "string",
                "description": "Additional details or context provided by the host for the item. Can be null. Sourced from BasicListItem.subtitle.",
                "optional": true
              }
            }
          }
        }
      }
    }
  },
  "legal_disclaimer": {
    "type": "string",
    "description": "Legal disclaimers or requirements (e.g., 'Must provide ID upon check-in')",
    "optional": true
  },
  "host": {
    "id": { "type": "string", "description": "Unique host identifier (e.g., '2855696')" },
    "name": { "type": "string", "description": "Host’s first name (e.g., 'Vanessa')" },
    "is_superhost": { "type": "boolean", "description": "Superhost status (e.g., true)" },
    "verification_status": {
      "type": "array",
      "description": "Host verification methods (e.g., ['Email', 'Phone', 'Government ID'])",
      "items": { "type": "string" },
      "optional": true
    },
    "languages_spoken": {
      "type": "array",
      "description": "Languages spoken by the host (e.g., ['English', 'French'])",
      "items": { "type": "string" },
      "optional": true
    },
    "contact_methods": {
      "type": "array",
      "description": "Methods to contact the host (e.g., ['Airbnb messaging', 'Phone', 'Email'])",
      "items": { "type": "string" },
      "optional": true
    },
    "years_hosting": { "type": "integer", "description": "Number of years hosting (e.g., 13)", "optional": true },
    "avatar": {
      "url": { "type": "string", "description": "URL of host’s profile picture" },
      "accessibility_label": { "type": "string", "description": "Alt text for avatar", "optional": true }
    },
    "bio": { "type": "string", "description": "Host biography", "optional": true },
    "response_rate": {
      "type": "number",
      "description": "Percentage of inquiries responded to (e.g., 98)",
      "optional": true
    },
    "response_time": {
      "value": { "type": "number", "description": "Average response time value (e.g., 1)", "optional": true },
      "unit": {
        "type": "string",
        "description": "Unit of response time (e.g., 'hours', 'days')",
        "optional": true,
        "default": "hours"
      },
      "note": "Added unit for clarity and flexibility"
    },
    "host_highlights": {
      "type": "array",
      "description": "List of host highlights e.g. ['Where I went to school: Kreisgymnasium Meppen, Freie Uni Berlin', 'Speaks English and German', 'Business']",
      "items": { "type": "string", "description": "Host highlight" },
      "optional": true
    },
    "business_details": {
      "type": "string",
      "description": "Business details (e.g., 'Business address, business registration number, business email, business phone number')",
      "optional": true
    },
    "co_hosts": {
      "type": "array",
      "description": "List of co-hosts",
      "items": {
        "id": { "type": "string", "description": "Co-host ID" },
        "name": { "type": "string", "description": "Co-host name" },
        "rating": { "type": "number", "description": "Co-host average rating (e.g., 4.86)", "optional": true }
      },
      "optional": true
    }
  },
  "capacity": {
    "guests": { "type": "integer", "description": "Maximum number of guests (e.g., 2)" },
    "bedrooms": { "type": "integer", "description": "Number of bedrooms (e.g., 0 for Studio, 1 for 1 bedroom)" },
    "room_type": {
      "type": "string",
      "description": "Type of room (e.g. entire_home', 'private_room', 'shared_room', 'hotel_room')",
      "optional": true
    },
    "beds": { "type": "integer", "description": "Number of beds (e.g., 1)" },
    "bed_types": {
      "type": "array",
      "description": "Types of beds (e.g., ['Queen bed', 'Sofa bed'])",
      "items": { "type": "string" },
      "optional": true,
      "source": "Airbnb’s 'Sleeping Arrangements'"
    },
    "sleeping_arrangements": {
      "type": "string",
      "description": "Description of sleeping arrangements (e.g., '1 queen bed, 1 sofa bed')",
      "optional": true,
      "source": "Airbnb’s 'Sleeping Arrangements' section"
    },
    "bathrooms": {
      "full_bathrooms": { "type": "integer", "description": "Number of full bathrooms (e.g., 1)", "optional": true },
      "half_bathrooms": { "type": "integer", "description": "Number of half bathrooms (e.g., 0)", "optional": true }
    },
    "square_meters": {
      "type": "number",
      "description": "Size of the property in square meters or feet",
      "optional": true
    },
    "occupancy_limit": {
      "type": "integer",
      "description": "Maximum occupants allowed by local regulations",
      "optional": true
    }
  },
  "pricing": {
    "base_price": { "type": "number", "description": "Base price per night (e.g., 280)" },
    "currency": { "type": "string", "description": "Currency code (e.g., 'USD')" },
    "currency_symbol": {
      "type": "string",
      "description": "Currency symbol for display (e.g., '$')",
      "note": "Made non-optional for display consistency"
    },
    "cleaning_fee": { "type": "number", "description": "Cleaning fee if applicable (e.g., 50)", "optional": true },
    "service_fee": { "type": "number", "description": "Airbnb service fee", "optional": true },
    "security_deposit": { "type": "number", "description": "Security deposit amount (e.g., 200)", "optional": true },
    "taxes": {
      "type": "object",
      "description": "Breakdown of applicable taxes (e.g., { 'VAT': 10, 'City Tax': 5 })",
      "additionalProperties": { "type": "number", "description": "Tax percentage or amount" },
      "optional": true
    },
    "tax_breakdown": {
      "type": "object",
      "description": "Detailed breakdown of taxes (e.g., { 'VAT': 10, 'City Tax': 5 })",
      "additionalProperties": { "type": "number", "description": "Tax percentage or amount" },
      "optional": true,
      "note": "Redundant with taxes; kept for clarity"
    },
    "additional_guest_fee": {
      "type": "number",
      "description": "Fee for extra guests beyond base capacity",
      "optional": true
    },
    "discounts": {
      "weekly": { "type": "number", "description": "Percentage discount for weekly stays", "optional": true },
      "monthly": { "type": "number", "description": "Percentage discount for monthly stays", "optional": true }
    },
    "dynamic_pricing": {
      "enabled": { "type": "boolean", "description": "Indicates if dynamic pricing is used", "optional": true },
      "tool": { "type": "string", "description": "Name of pricing tool (e.g., 'PriceLabs')", "optional": true }
    }
  },
  "availability": {
    "can_instant_book": { "type": "boolean", "description": "Instant book availability (e.g., false)" },
    "calendar": {
      "type": "array",
      "description": "Availability dates",
      "items": {
        "date": { "type": "string", "description": "Date in YYYY-MM-DD format" },
