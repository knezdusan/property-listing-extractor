{
  "host": {
    "id": { "type": "string", "description": "Unique host identifier (e.g., '2855696')" },
    "name": { "type": "string", "description": "Host’s first name (e.g., 'Vanessa')" },
    "superhost": { "type": "boolean", "description": "Superhost status (e.g., true)" },
    "photo": { "type": "string", "description": "URL of host’s profile picture" },
    "reviews": { "type": "integer", "description": "Number of reviews (e.g., 251)" },
    "rating": { "type": "number", "description": "Host’s average rating (e.g., 4.93)" },
    "years_hosting": { "type": "integer", "description": "Number of years hosting (e.g., 13)", "optional": true },
    "about": { "type": "string", "description": "Host’s about section (e.g., '')" },
    "highlights": {
      "type": "array",
      "description": "List of host highlights (e.g., ['Where I went to school: Kreisgymnasium Meppen, Freie Uni Berlin', 'Speaks English and German', 'Business'])",
      "items": { "type": "string" },
      "optional": true
    },
    "details": {
      "type": "array",
      "description": "List of host details (e.g., ['Response rate: 100%', 'Responds within an hour'])",
      "items": { "type": "string" },
      "optional": true
    },
    "contact_methods": {
      "type": "array",
      "description": "Methods to contact the host (e.g., ['Airbnb messaging', 'Phone', 'Email'])",
      "items": { "type": "string" },
      "optional": true
    },
    "business_details": {
      "type": "string",
      "description": "Business details (e.g., 'Business address, business registration number, business email, business phone number')",
      "optional": true
    },
    "cohosts": {
      "type": "array",
      "description": "List of co-hosts",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "description": "Co-host ID" },
          "name": { "type": "string", "description": "Co-host name" },
          "rating": { "type": "number", "description": "Co-host average rating (e.g., 4.86)", "optional": true }
        }
      },
      "optional": true
    },
    "other_listings": {
      "type": "array",
      "description": "List of other listings by the host",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "description": "Listing ID" },
          "name": { "type": "string", "description": "Listing name" },
          "url": { "type": "string", "description": "Listing URL" }
        }
      },
      "optional": true
    }
  },
  "listing": {
    "id": { "type": "string", "description": "Unique identifier for the listing (e.g., '30397973')" },
    "url": {
      "type": "string",
      "description": "Canonical URL of the listing (e.g., 'https://www.airbnb.com/rooms/30397973')"
    },
    "type": {
      "type": "string",
      "description": "Type of building or structure the listing is. eg.( 'apartment', 'house', 'bungalow', 'chalet', 'cabin', 'cottage', 'farmhouse', 'guesthouse', 'guestroom', 'houseboat', 'hut', 'igloo', 'loft', 'mansion', 'pavilion', 'shack', 'studio', 'tent', 'townhouse', 'villa', 'yurt' )"
    },
    "privacy": {
      "type": "string",
      "description": "what guests are renting (whole property or part) (e.g., 'Entire place', 'Private room', 'Shared room', 'Hotel room')"
    },
    "hero": {
      "type": "string",
      "description": "Hero image ID"
    },
    "title": {
      "type": "string",
      "description": "Listing title (e.g., 'Eiffel Tower IN your living room')",
      "constraints": "max 50 characters"
    },
    "subtitle": { "type": "string", "description": "Listing subtitle (e.g., 'Entire rental unit in Berlin, Germany')" },
    "capacity": {
      "type": "array",
      "description": "Listing capacity highlights (e.g., ['3 guests', '1 bedroom', '2 beds', '1 bath'])",
      "items": { "type": "string" }
    },
    "sleeping_arrangements": {
      "type": "array",
      "description": "Description of sleeping arrangements with rooms, beds and photo",
      "optional": true,
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string", "description": "Room type (e.g., 'Bedroom')" },
          "subtitle": { "type": "string", "description": "Bed type (e.g., '1 queen bed')", "optional": true },
          "images": {
            "type": "array",
            "items": { "type": "number", "description": "Image ID" },
            "optional": true
          }
        }
      }
    },
    "highlights": {
      "type": "array",
      "description": "Key selling points or notable features highlighted for the listing (e.g., Superhost status, great check-in, location perks).",
      "optional": true,
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "The main text of the highlight (e.g., 'Great check-in experience')."
          },
          "subtitle": {
            "type": "string",
            "description": "Supporting detail or explanation for the highlight.",
            "optional": true
          }
        },
        "required": ["title"]
      }
    },
    "description": { "type": "text", "description": "Provides in-depth property listing info for guests" },
    "average_daily_rate": { "type": "number", "description": "Average daily rate of the listing in USD (e.g., 165.5)" },
    "tags": {
      "type": "array",
      "description": "Custom tags for categorization (e.g., ['Romantic', 'Family-friendly', 'Business'])",
      "items": { "type": "string" },
      "optional": true
    }
  },
  "location": {
    "city": { "type": "string", "description": "City of the listing (e.g., 'Paris')" },
    "state": { "type": "string", "description": "Region or state (e.g., 'Île-de-France')" },
    "country": { "type": "string", "description": "Country (e.g., 'France')" },
    "address": {
      "type": "string",
      "description": "Address of the listing (e.g., '2 Place des Abbesses')",
      "optional": true
    },
    "zip": { "type": "string", "description": "Zip code of the listing (e.g., '75008')", "optional": true },
    "coordinates": { "type": "string", "description": "Coordinates of the listing (e.g., '48.85068,2.29188')" },
    "details": {
      "type": "array",
      "description": "Detailed information about the neighborhood, including a preview and potentially extended sections like 'Getting Around'.",
      "optional": true,
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string", "description": "Title for this section (e.g., 'Getting around')." },
          "content": { "type": "string", "description": "Text content for this section." }
        }
      }
    },
    "disclaimer": { "type": "string", "description": "Location disclaimer", "optional": true }
  },
  "house_rules": {
    "intro": {
      "type": "array",
      "description": "List of house rules intro items, eg. ['Check-in: 4:00 PM - 11:00 PM', 'Checkout before 11:00 AM', '3 guests maximum']",
      "items": { "type": "string" }
    },
    "sections": {
      "type": "array",
      "description": "Rules guests must follow devided by sections",
      "items": {
        "type": "object",
        "description": "Rules section",
        "properties": {
          "section": { "type": "string", "description": "Title of the rules section" },
          "rules": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": { "type": "string" },
                "subtitle": { "type": "string" },
                "html": { "type": "string", "description": "HTML content for the rule", "optional": true }
              }
            }
          }
        }
      }
    }
  },
  "safety_features": {
    "intro": {
      "type": "array",
      "description": "List of safety & property intro items, eg. ['Exterior security cameras on property', '24/7 security']",
      "items": { "type": "string" }
    },
    "sections": {
      "type": "array",
      "description": "Safety and property features devided by sections",
      "items": {
        "type": "object",
        "description": "Safety and property features sections",
        "properties": {
          "title": { "type": "string", "description": "Title of the safety and property features section" },
          "features": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": { "type": "string" },
                "subtitle": { "type": "string" },
                "html": { "type": "string", "description": "HTML content for the feature", "optional": true }
              }
            }
          }
        }
      }
    }
  },
  "amenities": {
    "type": "array",
    "items": {
      "category": { "type": "string", "description": "Category name (e.g., 'Bathroom')", "optional": true },
      "amenities": {
        "type": "array",
        "items": { "title": "string", "subtitle": "string", "top": false, "icon": "string", "available": true },
        "optional": true
      }
    }
  },
  "gallery": {
    "type": "object",
    "properties": {
      "photos": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": { "type": "string" },
            "baseUrl": { "type": "string" },
            "aspectRatio": { "type": "number", "optional": true },
            "orientation": { "type": "string", "optional": true },
            "accessibilityLabel": { "type": "string", "optional": true },
            "caption": { "type": "string", "optional": true }
          }
        },
        "description": "List of all property listing images with all details"
      },
      "tour": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": { "type": "string" },
            "photos": { "type": "array", "items": { "type": "number" } },
            "highlights": { "type": "array", "items": { "type": "string" } }
          }
        },
        "description": "Photo tour of property lising categorized by room type, eg. ['Living room', 'Bedroom 1', 'Bedroom 2', 'Kitchen', 'Bathroom']"
      }
    }
  },
  "availability": {
    "minNights": {
      "type": "integer",
      "description": "Minimum nights required for the listing (e.g., 1)",
      "optional": true
    },
    "booked": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "start": { "type": "string" },
          "end": { "type": "string" },
          "checkOut": { "type": "string" },
          "checkIn": { "type": "string" }
        }
      },
      "description": "List of booked date ranges"
    }
  },
  "category_ratings": {
    "accuracy": { "type": "number", "description": "Accuracy rating (e.g., 4.94)", "optional": true },
    "check_in": { "type": "number", "description": "Check-in rating (e.g., 4.95)", "optional": true },
    "cleanliness": { "type": "number", "description": "Cleanliness rating (e.g., 4.88)", "optional": true },
    "communication": { "type": "number", "description": "Communication rating (e.g., 4.98)", "optional": true },
    "location": { "type": "number", "description": "Location rating (e.g., 4.94)", "optional": true },
    "value": { "type": "number", "description": "Value rating (e.g., 4.75)", "optional": true },
    "guest_satisfaction": {
      "type": "number",
      "description": "Guest satisfaction rating (e.g., 4.96)",
      "optional": true
    }
  },
  "reviews": {
    "type": "array",
    "description": "List of individual reviews",
    "items": {
      "id": { "type": "string", "description": "Review ID" },
      "language": { "type": "string", "description": "Review language" },
      "comments": { "type": "string", "description": "Review content in english" },
      "rating": { "type": "number", "description": "Star rating for this review (e.g., 5)" },
      "highlight": { "type": "string", "description": "Review highlight, eg. 'Stayed one night'", "optional": true },
      "period": { "type": "string", "description": "Visit period, eg. 'June 2023'", "optional": true },
      "reviewer": {
        "type": "object",
        "description": "Reviewer information",
        "properties": {
          "id": { "type": "string", "description": "Reviewer ID" },
          "name": { "type": "string", "description": "Reviewer first name" },
          "photo": { "type": "string", "description": "Reviewer photo URL" }
        }
      },
      "response": { "type": "string", "description": "Host’s response to the review", "optional": true },
      "createdAt": {
        "type": "string",
        "description": "Review date in YYYY-MM-DDTHH:mm:ssZ format (e.g., '2023-05-30T13:32:06Z')"
      }
    },
    "optional": true
  },
  "legal_disclaimer": {
    "type": "string",
    "description": "Legal disclaimers or requirements (e.g., 'Must provide ID upon check-in')",
    "optional": true
  },
  "pricing": {
    "base_price": { "type": "number", "description": "Base price per night (e.g., 280)" },
    "currency": { "type": "string", "description": "Currency code (e.g., 'USD')" },
    "currency_symbol": {
      "type": "string",
      "description": "Currency symbol for display (e.g., '$')",
      "note": "Made non-optional for display consistency"
    },
    "cleaning_fee": { "type": "number", "description": "Cleaning fee if applicable (e.g., 50)", "optional": true },
    "service_fee": { "type": "number", "description": "Airbnb service fee", "optional": true },
    "security_deposit": { "type": "number", "description": "Security deposit amount (e.g., 200)", "optional": true },
    "taxes": {
      "type": "object",
      "description": "Breakdown of applicable taxes (e.g., { 'VAT': 10, 'City Tax': 5 })",
      "additionalProperties": { "type": "number", "description": "Tax percentage or amount" },
      "optional": true
    },
    "tax_breakdown": {
      "type": "object",
      "description": "Detailed breakdown of taxes (e.g., { 'VAT': 10, 'City Tax': 5 })",
      "additionalProperties": { "type": "number", "description": "Tax percentage or amount" },
      "optional": true,
      "note": "Redundant with taxes; kept for clarity"
    },
    "additional_guest_fee": {
      "type": "number",
      "description": "Fee for extra guests beyond base capacity",
      "optional": true
    },
    "discounts": {
      "weekly": { "type": "number", "description": "Percentage discount for weekly stays", "optional": true },
      "monthly": { "type": "number", "description": "Percentage discount for monthly stays", "optional": true }
    },
    "dynamic_pricing": {
      "enabled": { "type": "boolean", "description": "Indicates if dynamic pricing is used", "optional": true },
      "tool": { "type": "string", "description": "Name of pricing tool (e.g., 'PriceLabs')", "optional": true }
    }
  },
  "cancellation_policy": {
    "type": { "type": "string", "description": "Cancellation policy type (e.g., 'Strict')" },
    "policy_id": { "type": "string", "description": "Policy identifier (e.g., '44')", "optional": true },
    "details": { "type": "string", "description": "Policy description", "optional": true },
    "refund_percentage": {
      "type": "object",
      "description": "Refund percentages for different timeframes (e.g., { '7_days': 50, '14_days': 75 })",
      "additionalProperties": { "type": "number", "description": "Percentage refundable" },
      "optional": true
    },
    "grace_period": {
      "type": "string",
      "description": "Grace period for cancellations (e.g., '24 hours')",
      "optional": true,
      "source": "Airbnb’s cancellation policy options"
    },
    "link": {
      "type": "string",
      "description": "URL to full policy details (e.g., 'https://www.airbnb.com/help/article/2637')",
      "optional": true
    }
  },
  "seo": {
    "meta_description": { "type": "string", "description": "SEO meta description", "optional": true },
    "og_tags": {
      "title": { "type": "string", "description": "Open Graph title", "optional": true },
      "description": { "type": "string", "description": "Open Graph description", "optional": true },
      "image": { "type": "string", "description": "Open Graph image URL", "optional": true },
      "type": {
        "type": "string",
        "description": "Open Graph type (e.g., 'airbedandbreakfast:listing')",
        "optional": true
      },
      "url": { "type": "string", "description": "Open Graph URL", "optional": true }
    },
    "twitter_tags": {
      "card": { "type": "string", "description": "Twitter card type (e.g., 'photo')", "optional": true },
      "image": { "type": "string", "description": "Twitter image URL", "optional": true },
      "url": { "type": "string", "description": "Twitter URL", "optional": true }
    },
    "breadcrumbs": {
      "type": "array",
      "description": "SEO breadcrumb navigation",
      "items": {
        "link_route": { "type": "string", "description": "URL route (e.g., '/paris-france/stays')" },
        "text": { "type": "string", "description": "Display text (e.g., 'Paris')" },
        "search_text": { "type": "string", "description": "Search term (e.g., 'Paris, France')", "optional": true }
      },
      "optional": true
    }
  },
  "logging_context": {
    "page": { "type": "string", "description": "Page type (e.g., 'p3')", "optional": true },
    "pdp_page_type": { "type": "integer", "description": "PDP page type (e.g., 1)", "optional": true },
    "impression_id": {
      "type": "string",
      "description": "PDP impression ID (e.g., 'p3_1742313065_P36QaVz7NVuACUde')",
      "optional": true
    },
    "description_language": {
      "type": "string",
      "description": "Language of description (e.g., 'en')",
      "optional": true
    }
  },
  "additional_features": {
    "sustainability": {
      "type": "array",
      "description": "Eco-friendly features (e.g., ['Solar power', 'Recycling'])",
      "items": { "type": "string" },
      "optional": true
    },
    "pet_friendly": { "type": "boolean", "description": "Indicates if pets are allowed", "optional": true },
    "kid_friendly": {
      "type": "boolean",
      "description": "Indicates if the property is suitable for children",
      "optional": true
    },
    "parking": {
      "type": "array",
      "description": "Parking options (e.g., ['Free street parking', 'Garage'])",
      "items": { "type": "string" },
      "optional": true
    },
    "accessibility": {
      "type": "array",
      "description": "Accessibility features (e.g., ['Wheelchair accessible', 'Step-free entry'])",
      "items": { "type": "string" },
      "optional": true
    },
    "smart_home": {
      "type": "array",
      "description": "Smart home features (e.g., ['Keyless entry', 'Smart thermostat'])",
      "items": { "type": "string" },
      "optional": true
    },
    "views": {
      "type": "array",
      "description": "Views from the property (e.g., ['City view', 'Mountain view'])",
      "items": { "type": "string" },
      "optional": true
    },
    "outdoor_space": {
      "type": "array",
      "description": "Outdoor spaces (e.g., ['Balcony', 'Garden', 'Patio'])",
      "items": { "type": "string" },
      "optional": true
    },
    "unique_experiences": {
      "type": "array",
      "description": "Unique experiences offered (e.g., ['Cooking class', 'Wine tasting'])",
      "items": { "type": "string" },
      "optional": true,
      "source": "Airbnb’s 'Experiences' integration with stays"
    },
    "local_customs": {
      "type": "array",
      "description": "Local customs or traditions (e.g., ['Tea ceremony', 'Local festivals'])",
      "items": { "type": "string" },
      "optional": true,
      "source": "Airbnb’s 'About This Space' or host-provided cultural notes"
    }
  }
}
